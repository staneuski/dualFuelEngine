FoamFile
{
    version     2.0;
    format      ascii;
    class       pointScalarField;
    location    "0";
    object      pointLevel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inletXMinus
    {
        type            calculated;
    }
    inletXPlus
    {
        type            calculated;
    }
    inletYMinus
    {
        type            calculated;
    }
    inletYPlus
    {
        type            calculated;
    }
    outlet
    {
        type            calculated;
    }
    valve
    {
        type            calculated;
    }
    walls
    {
        type            calculated;
    }
}


