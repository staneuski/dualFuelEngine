FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)            cellLimited Gauss linear 0.9; // UEqn
}

divSchemes
{
    default         Gauss cubic;
    div(phi,U)      Gauss upwind; // UEqn
    div(phi,e)      Gauss upwind; // eEqn
    div(phi,alphaAir) Gauss upwind; // alphaAirEqn
    div(phi,alphaGas) Gauss upwind; // alphaGasEqn
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         cubic;
}

snGradSchemes
{
    default         corrected;
}

// ************************************************************************* //