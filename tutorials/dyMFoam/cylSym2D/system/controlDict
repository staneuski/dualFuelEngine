FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application          dyMFoam;//pimpleFoam;

startFrom            startTime;

startTime            0;

stopAt               endTime;

endTime              0.5;

deltaT               0.01;//5e-03;

writeControl         adjustableRunTime;

writeInterval        0.01;//5e-03;

purgeWrite           0;

writeFormat          ascii;

writePrecision       6;

writeCompression     off;

timeFormat           general;

timePrecision        6;

runTimeModifiable    true;

adjustTimeStep       true;

maxCo                0.3;

