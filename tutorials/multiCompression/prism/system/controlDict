FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     	multiCompression;

startFrom       	startTime;

startTime       	0;

stopAt          	endTime;

endTime         	0.07;

deltaT          	1e-03;

writeControl    	timeStep;

writeInterval   	10;

purgeWrite      	0;

writeFormat     	ascii;

writePrecision  	6;

writeCompression 	off;

timeFormat      	general;

timePrecision   	6;

runTimeModifiable 	true;


