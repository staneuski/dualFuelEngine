FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     		multiCompression;

startFrom       		startTime;

startTime       		0;

stopAt          		endTime;

endTime         		2e-02;

deltaT          		5e-06;

writeControl    		timeStep;

writeInterval   		20; // 5e-06*10 = .00001

purgeWrite      		0;

writeFormat     		ascii;

writePrecision  		6;

writeCompression 		on;

timeFormat      		general;

timePrecision   		6;

runTimeModifiable		true;

