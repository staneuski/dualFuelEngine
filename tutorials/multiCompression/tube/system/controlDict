FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application          multiCompression;

startFrom            startTime;

startTime            0;

stopAt               endTime;

endTime              3e-02;

deltaT               2e-05;

writeControl         timeStep;

writeInterval        5;

purgeWrite           0;

writeFormat          ascii;

writePrecision       6;

writeCompression     off;

timeFormat           general;

timePrecision        6;

runTimeModifiable    true;

// ************************************************************************* //
