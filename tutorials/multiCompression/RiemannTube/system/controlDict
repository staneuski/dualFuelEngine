FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     	  multiCompression;

startFrom       	  startTime;

startTime       	  0;

stopAt          	  endTime;

endTime         	  7e-04;

deltaT          	  1.5e-05; //Co < 0.5 => deltaT < 0.5*deltaX/a = 0.5*1e-02/342

writeControl    	  timeStep;

writeInterval   	  1;

purgeWrite      	  0;

writeFormat     	  ascii;

writePrecision  	  6;

writeCompression 	  off;

timeFormat      	  general;

timePrecision   	  6;

runTimeModifiable 	true;


