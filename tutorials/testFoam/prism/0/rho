FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1.204;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1.204;
		
    }
    outlet
    {
        type            zeroGradient;
    }

    walls
    {
        type            zeroGradient;
    }
}


