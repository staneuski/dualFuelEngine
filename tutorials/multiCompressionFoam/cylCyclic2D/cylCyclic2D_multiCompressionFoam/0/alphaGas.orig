FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphaGas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
/* Inlet */
    injection
    {
        type            fixedValue;
        value           uniform 1;
    }
    inlet
    {
        type            zeroGradient;
    }

/* Outlet */
    outlet
    {
        type            zeroGradient;
    }

/* Walls */
    "piston|valve(Head|Stem)|walls"
    {
        type            zeroGradient;
    }

/* ACMI & cyclic patches */
    #includeEtc "caseDicts/setConstraintTypes"

    "(inner|outer)ACMI(1|2)_blockage"
    {
        type            zeroGradient;
    }
    "(inner|outer)ACMI(1|2)_couple"
    {
        type            cyclicACMI;
        value           $internalField;
    }
}

// ************************************************************************* //
