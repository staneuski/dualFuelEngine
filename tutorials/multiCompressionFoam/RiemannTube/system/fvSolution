FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(rho|U).*"
    {
        solver          diagonal;
    }

    "e.*"
    {
        solver          GAMG;
        smoother        DILU;
        tolerance       1e-06;
        relTol          0.01;
    }

    "(alphaAir|alphaGas).*"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-06;
        relTol          0;
    }
    "cellMotionU.*"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-08;
        relTol          0;
    }
}

PIMPLE
{
    nOuterCorrectors    2; // No of iterations
    correctPhi          false;
    checkMeshCourantNo  false;
}

relaxationFactors
{
    fields
    {
        p               0.5;
    }
    equations
    {
        U               1;
        e               1;
    }
}

// ************************************************************************* //
