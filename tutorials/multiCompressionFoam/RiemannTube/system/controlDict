FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     multiCompressionFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.0125;

deltaT          3e-05;

writeControl    adjustableRunTime;

writeInterval   5e-05;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression false;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  true;

maxCo           0.1;

functions
{
    #includeFunc residuals //$ foamMonitor -title "Residuals" $FOAM_ADD/dualFuelEngine/tutorials/multiCompressionFoam/RiemannTube/postProcessing/residuals/0/residuals.dat
}


