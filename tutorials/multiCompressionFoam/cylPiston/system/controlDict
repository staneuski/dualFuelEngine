FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     multiCompressionFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         5e-02;

deltaT          1e-04;

writeControl    adjustableRunTime;

writeInterval   1e-03;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression false;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  true;

maxCo           0.05;

