FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0]; // [Pa]

internalField   uniform 1e+06;

boundaryField
{
    // Injection
    "injection(X|Y)(Minus|Plus)"
    {
        type            zeroGradient;
    }

    // Inlet
    "inlet(X|Y)(Minus|Plus)"
    {
        $injectionXMinus
    }

    // Outlet
    outlet
    {
        type            fixedValue;
        value           uniform 0.23e+06;
    }

    // Walls
    "piston|valve|walls"
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
